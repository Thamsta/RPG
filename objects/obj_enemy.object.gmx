        rdir = point_direction(x,y,sx,sy)+random(20)-10;
   }
   hspd = lengthdir_x(0.5, rdir);
   vspd = lengthdir_y(0.5, rdir);
}
     
//move

phy_position_x += hspd;
phy_position_y += vspd;
if(hspd&gt;0)
{
    image_xscale = -1;
}else
{
